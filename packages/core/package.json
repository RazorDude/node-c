    },
    "./persistance/entityService": {
      "types": "./src/persistance/entityService/index.ts",
      "default": "./dist/persistance/entityService/index.js"
    }
  },
  "dependencies": {
    "@nestjs/common": "^10.4.12",
    "@nestjs/core": "^10.4.12",
    "@ramster/general-tools": "^2.3.0",
    "morgan": "^1.10.0",
    "ramda": "^0.30.1"
  },
  "devDependencies": {
    "@repo/config-typescript": "*",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.10.1",
    "@types/ramda": "^0.30.2",
    "typescript": "latest"
  }
