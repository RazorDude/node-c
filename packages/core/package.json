{
  "name": "@node-c/core",
  "version": "1.0.0-alpha30",
  "license": "MIT",
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "build:clean": "rm -rf dist/* && rm -f *.tsbuildinfo && npm run build",
    "check-types": "tsc -p tsconfig.build.json --noEmit",
    "dev": "tsc -p tsconfig.build.json --watch",
    "publish-package": "npm run build:clean && npm publish --access public",
    "test": "vitest --config src/vitest.config.ts",
    "test:coverage": "vitest --config src/vitest.config.ts --coverage"
  },
  "dependencies": {
    "@nestjs/common": "^10.4.12",
    "@nestjs/core": "^10.4.12",
    "dotenv": "^16.4.7",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "ramda": "^0.30.1"
  },
  "devDependencies": {
    "@nestjs/platform-express": "^10.4.15",
    "@types/lodash": "^4.17.19",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.10.1",
    "@types/ramda": "^0.30.2",
    "@vitest/coverage-v8": "^3.0.5",
    "immutable": "^5.0.3",
    "vitest": "^3.0.5"
  }
}
