{
  "name": "@repo/core",
  "version": "1.0.0-alpha0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc"
  },
  "exports": {
    "./app": {
      "types": "./src/app.ts",
      "default": "./dist/app.js"
    },
    "./common/configProvider": {
      "types": "./src/common/configProvider/index.ts",
      "default": "./dist/common/configProvider/index.js"
    },
    "./common/definitions": {
      "types": "./src/common/definitions/index.ts",
      "default": "./dist/common/definitions/index.js"
    },
    "./common/utils": {
      "types": "./src/common/utils/index.ts",
      "default": "./dist/common/utils/index.js"
    },
    "./domain/entityService": {
      "types": "./src/domain/entityService/index.ts",
