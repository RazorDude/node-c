{
  "name": "node-c",
  "private": true,
  "version": "1.0.0-alpha0",
  "description": "An end-to-end NodeJS framework for building DDD apps built on top of NestJS.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": {
    "name": "Rumen Rumenov",
    "email": "mmetalonlyy@gmail.com"
  },
  "engines": {
    "node": ">= 22"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "./node_modules/.bin/tsc --project tsconfig.build.json",
    "build:watch": "./node_modules/.bin/tsc --project tsconfig.build.json --watch",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start:debug": "NODE_ENV=local nest start --path=tsconfig.test.app.json --debug --watch",
    "start:local": "NODE_ENV=local nest start --path=tsconfig.test.app.json --watch",
    "start:dev-online": "NODE_ENV=dev node dist/main",
    "start:staging": "NODE_ENV=staging node dist/main",
    "start:prod": "NODE_ENV=prod node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "migration:run": "npm run build && npm run generateOrmconfig && npm run typeorm migration:run",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js"
  },
  "dependencies": {
    "@nestjs/common": "^10.4.12",
    "@nestjs/core": "^10.4.12",
    "@nestjs/platform-express": "^10.4.12",
    "@nestjs/swagger": "^8.0.7",
    "@nestjs/typeorm": "^10.0.2",
    "@ramster/general-tools": "^2.3.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.7",
    "dotenv": "^16.4.5",
    "jsonwebtoken": "^9.0.2",
    "luxon": "^3.5.0",
    "morgan": "^1.10.0",
    "ramda": "^0.30.1",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.8",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "^10.4.12",
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.8",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/luxon": "^3.4.2",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.10.1",
    "@types/ramda": "^0.30.2",
    "@typescript-eslint/eslint-plugin": "^8.16.0",
    "@typescript-eslint/parser": "^8.16.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "prettier": "^3.4.1",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "pg": "^8.13.1",
    "redis": "^4.7.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "modulePaths": [
      "./"
    ],
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
