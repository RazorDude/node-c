{
  "name": "@node-c/app-test",
  "version": "1.0.0-alpha64",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "build:clean": "rm -rf dist/* && rm -f *.tsbuildinfo && npm run build",
    "check-types": "tsc -p tsconfig.build.json --noEmit",
    "lint": "eslint src",
    "start:dev": "nest start src/main.ts --watch",
    "start:test": "NODE_ENV=test nest start src/main.ts --watch",
    "test": "NODE_ENV=test vitest --config test/vitest.config.ts",
    "test:coverage": "NODE_ENV=test vitest --config test/vitest.config.ts --coverage",
    "typeorm": "typeorm-ts-node-commonjs",
    "typeorm:migration:generate:db": "npm run typeorm -- migration:generate -d datasource-db.ts $MIGRATION_NAME",
    "typeorm:migration:run:db": "npm run typeorm -- migration:run -d datasource-db.ts"
  },
  "dependencies": {
    "@nestjs/common": "^10.4.12",
    "cookie-parser": "^1.4.7",
    "express": "^4.21.2",
    "mysql2": "^3.14.2",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.14",
    "@types/cookie-parser": "^1.4.8",
    "@types/express": "^5.0.0",
    "nestjs-spelunker": "^1.3.2",
    "ts-node": "^10.9.2"
  },
  "peerDependencies": {
    "@node-c/api-http": "^1.0.0-alpha64",
    "@node-c/api-rest": "^1.0.0-alpha64",
    "@node-c/core": "^1.0.0-alpha64",
    "@node-c/domain-iam": "^1.0.0-alpha64",
    "@node-c/data-clickhouse": "^1.0.0-alpha64",
    "@node-c/data-redis": "^1.0.0-alpha64",
    "@node-c/data-typeorm": "^1.0.0-alpha64"
  }
}
